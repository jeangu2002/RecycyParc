{% extends "::base.html.twig" %}

{% block title %}AppBundle:Dechet:create{% endblock %}

{% block body %}
    <div class="container">
        <h1>Welcome to the Dechet:create page</h1>
        <div class="col-md-6">
            {{ form_start(form, {'attr': {'class': 'my-form-class'} }) }}
            {{ form_row(form._token) }}
         <div class="form-group">
            {{ form_label(form.nom)}}
            {{ form_widget(form.nom, {'attr': {'class': 'form-control'}})}}
            {{ form_errors(form.nom) }}
        </div>
        <div class="form-group">
            {{ form_label(form.volumeDeBase)}}
            {{ form_widget(form.volumeDeBase, {'attr': {'class': 'form-control'}})}}
            {{ form_errors(form.volumeDeBase) }}
        </div>
        <div class="form-group">
            {{ form_label(form.quota)}}
            {{ form_widget(form.quota, {'attr': {'class': 'form-control'}})}}
            {{ form_errors(form.quota) }}
        </div>
        <input type="submit" value="Enregistrer" class="btn btn-success pull-right" />
        {{ form_end(form,{'render_rest': false}) }}
       </div>
    </div>
{% endblock %}
