{% extends "::base.html.twig" %}

{% block title %}AppBundle:Conteneur:create{% endblock %}

{% block body %}
    <div class="container">
        <div class="row">
            <h1>Welcome to the Conteneur:create page</h1>
            
            <div class="col-md-6">
                {{ form_start(form, {'attr': {'class': 'my-form-class'} }) }}
                {{ form_row(form._token) }}
                <div class="form-group">
                    {{ form_label(form.contenance)}}
                    {{ form_widget(form.contenance, {'attr': {'class': 'form-control'}})}}
                    {{ form_errors(form.contenance) }}
                </div>
                <div class="form-group">
                    {{ form_label(form.emplacement)}}
                    {{ form_widget(form.emplacement, {'attr': {'class': 'form-control'}})}}
                    {{ form_errors(form.emplacement) }}
                </div>
                 <div class="form-group">
                    {{ form_label(form.parc)}}
                    {{ form_widget(form.parc, {'attr': {'class': 'form-control'}})}}
                    {{ form_errors(form.parc) }}
                </div>
                <div class="form-group">
                    {{ form_label(form.dechets)}}
                    {{ form_widget(form.dechets, {'attr': {'class': 'form-control'}})}}
                    {{ form_errors(form.dechets) }}
                </div>
                <input type="submit" value="Enregistrer" class="btn btn-success pull-right" />
               {{ form_end(form,{'render_rest': false}) }}
           </div>
          
      </div>
    </div>
{% endblock %}
